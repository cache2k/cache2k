 xsi:noNamespaceSchemaLocation="file:../../main/resources/cache2kv2.xsd">
    <version>1.0</version>
    <defaultManagerName>default</defaultManagerName>
    <cacheManager>
        <managerName>default</managerName>
        <classLoader>org.cache.MyClassLoader</classLoader>
        <isDefaultManager>true</isDefaultManager>
    </cacheManager>
    <ignoreAnonymousCache>false</ignoreAnonymousCache>
    <skipCheckOnStartup>false</skipCheckOnStartup>
    <properties>
        <user>
             <smallCacheCapacity>12_000</smallCacheCapacity>
      		 <userHome>${ENV.HOME}</userHome>
        </user>
    </properties>
    <defaults>
        <cache>
            <name>name_0</name>
            <entryCapacity>2000</entryCapacity>
            <expireAfterWrite>-1</expireAfterWrite>
            <resilienceDuration>-1</resilienceDuration>
            <keyType>
                <type>org.cache.KeyType</type>
            </keyType>
            <valueType>
                <type>org.cache.ValueType</type>
            </valueType>
            <storeByReference>false</storeByReference>
            <strictEviction>false</strictEviction>
            <refreshAhead>false</refreshAhead>
            <retryInterval>-1</retryInterval>
            <maxRetryInterval>-1</maxRetryInterval>
            <keepDataAfterExpired>false</keepDataAfterExpired>
            <sharpExpiry>false</sharpExpiry>
            <suppressExceptions>true</suppressExceptions>
            <loaderThreadCount>2</loaderThreadCount>
            <permitNullValues>false</permitNullValues>
            <disableStatistics>false</disableStatistics>
            <disableLastModificationTime>false</disableLastModificationTime>
            <boostConcurrency>false</boostConcurrency>
            <eternal>true</eternal>
            <loader>
<!--                <byClassName>
                    <className></className>
                </byClassName>-->
                <bean>
                    <type>org.cache2k.MyType</type>
                    <className>MyClass</className>
                </bean>
            </loader>
            <advancedLoader>
                <byClassName>
                    <className>org.cache2k.MyAdvancedLoaderClass</className>
                </byClassName>
<!--                <bean>
                    <type></type>
                    <className></className>
                </bean>-->
            </advancedLoader>
            <writer>
                <byClassName>
                    <className>org.cache2k.MyWriterClass</className>
                </byClassName>
<!--                <bean>
                    <type></type>
                    <className></className>
                </bean>-->
            </writer>
            <expiryPolicy>
<!--                <byClassName>
                    <className></className>
                </byClassName>-->
                <bean>
                    <type>org.cache2k.ExpiryPolicyClass</type>
                    <className>MyExpiryPolicyClass</className>
                </bean>
            </expiryPolicy>
            <resiliencePolicy>
<!--                <byClassName>
                    <className></className>
                </byClassName>-->
                <bean>
                    <type>org.cache2k.ResiliencePolicyClass</type>
                    <className>MyResiliencePolicyClass</className>
                </bean>
            </resiliencePolicy>
            <exceptionPropagator>
                <byClassName>
                    <className>MyExceptionPropagatorClass</className>
                </byClassName>
<!--                <bean>
                    <type></type>
                    <className></className>
                </bean>-->
            </exceptionPropagator>
            <loaderExecutor>
                <byClassName>
                    <className>MyLoaderExecutorClass</className>
                </byClassName>
<!--                <bean>
                    <type></type>
                    <className></className>
                </bean>-->
            </loaderExecutor>
            <prefetchExecutor>
                <byClassName>
                    <className>MyPrefetchExecutorClass</className>
                </byClassName>
<!--                <bean>
                    <type></type>
                    <className></className>
                </bean>-->
            </prefetchExecutor>
            <asyncListenerExecutor>
<!--                <byClassName>
                    <className></className>
                </byClassName>-->
                <bean>
                    <type>org.cache2k.AsyncListenerExecutorClass</type>
                    <className>MyAsyncListenerExecutorClass</className>
                </bean>
            </asyncListenerExecutor>
            <clock>
                <byClassName>
                    <className>MyClass</className>
                </byClassName>
<!--                <bean>
                    <type></type>
                    <className></className>
                </bean>-->
            </clock>
            <listeners>
                <listener>
                    <type>org.cache2k.ListenerClass</type>
                    <className>MyListenerClass</className>
                </listener>
            </listeners>
            <asyncListeners>
                <listener>
                    <type>org.cache2k.AsyncListenerClass</type>
                    <className>MyAsyncListenerClass</className>
                </listener>
            </asyncListeners>
            <closedListeners>
                <listener>
                    <type>org.cache2k.ClosedListenerClass</type>
                    <className>MyClosedListenerClass</className>
                </listener>
            </closedListeners>
            <sections>
<!--                <section>
                    <type>org.cache2k.jcache.JCacheConfiguration</type>
                    <copyAlwaysIfRequested></copyAlwaysIfRequested>
                </section>-->
                <jcache>
                    <alwaysFlushJmxStatistics>false</alwaysFlushJmxStatistics>
                    <copyAlwaysIfRequested>true</copyAlwaysIfRequested>
                </jcache>
            </sections>
            <include>include0,include1</include>
        </cache>
